x<-c(8.1,7.3,6.2,9.3,8.5,8.7,7.2,6.8,6.9,9.5)
y<-c(4,2,2.5,5,4.5,4.5,5,2,1,6)
plot(x,y, xlab = "Calificaciones", ylab = "Horas de Estudio", xlim = c(0, 10), ylim = c(0, 6),sub="Elaboración : Fuente propia",col="navy blue", main="Relación entre las calificaciones de los alumos y las horas que estudian ")
