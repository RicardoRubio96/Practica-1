vax<-c(132,129,120,113.2,105,92,84,83.2,88.4,59,80,81.5,71,69.2)
vay<-c(46,48,51,52.1,54,52,59,58.7,61.6,64,61.4,54.6,58.8,58)
mediax<-mean(vax)
mediay<-mean(vay)
sumax<-sum(vax)
sumay<-sum(vay)
mult<-vax*vay
sumamult<-sum(mult)
vax2<-vax*vax
sumavax2<-sum(vax2)
sumax2<-sumax*sumax
SXY<-sumamult-((sumax*sumay)/14)
SXX<-sumavax2-sumax2/14
beta1<-SXY/SXX
beta0<-mediay-beta1*mediax
valx<-c(115,138,55,99,290,70)
valy<-beta0+beta1*valx
valy
